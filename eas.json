{
  "cli": {
    "version": ">= 16.27.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "http://localhost:4000",
        "EXPO_PUBLIC_API_URL_ANDROID": "http://192.168.1.210:4000"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_STAGE": "prod",
        "EXPO_PUBLIC_API_URL": "https://criticast-backend.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}